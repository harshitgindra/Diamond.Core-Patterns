{
	//
	// Aliases are global and can be used anywhere.
	//
	"aliases": [
		{
			"Key": "IDoAction",
			"TypeDefinition": "Diamond.Core.AspNet.DoAction.IDoAction, Diamond.Core.AspNet.DoAction.Abstractions"
		}
	],
	//
	// If Lifetime is not specified, the default is Scoped.
	//
	"services": [
		{
			"ServiceType": "<IDoAction>",
			"ImplementationType": "Diamond.Core.Example.CreateInvoiceAsyncAction, WebExample",
			"Lifetime": "Scoped"
		},
		{
			"ServiceType": "<IDoAction>",
			"ImplementationType": "Diamond.Core.Example.DeleteInvoiceAsyncAction, WebExample",
			"Lifetime": "Scoped"
		},
		{
			"ServiceType": "<IDoAction>",
			"ImplementationType": "Diamond.Core.Example.GetInvoiceAsyncAction, WebExample",
			"Lifetime": "Scoped"
		},
		{
			"ServiceType": "<IDoAction>",
			"ImplementationType": "Diamond.Core.Example.GetAllInvoicesAsyncAction, WebExample",
			"Lifetime": "Scoped"
		},
		{
			"ServiceType": "<IDoAction>",
			"ImplementationType": "Diamond.Core.Example.UpdateInvoiceAsyncAction, WebExample",
			"Lifetime": "Scoped"
		},
		{
			"ServiceType": "<IDoAction>",
			"ImplementationType": "Diamond.Core.Example.MarkInvoicePaidAsyncAction, WebExample",
			"Lifetime": "Scoped"
		}
	]
}